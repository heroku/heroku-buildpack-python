import telebot
from telebot import types
bot = telebot.TeleBot("5278842162:AAFJ6EWNAOb8PXOwz8nKBlHbgdUkR8SU73k")

@bot.message_handler(commands=['start'])
def start(message):
	markup = types.ReplyKeyboardMarkup(resize_keyboard = True, row_width= 2)
	item1 = types.KeyboardButton('ü§ï –î–ª—è –Ω–æ–≤–∏—á–∫–æ–≤')
	item2 = types.KeyboardButton('üòé –î–ª—è –ø—Ä–æ–¥–≤–∏–Ω—É—Ç—ã—Ö')
	item3 = types.KeyboardButton('üìã FAQ')

	markup.add(item1, item2, item3)

	bot.send_message(message.chat.id,'&#128165<b>TeamFirestorm -</b> –∫—Ä—É–ø–Ω–µ–π—à–∏–π –≤ –°–ù–ì –ø–æ–∫–µ—Ä–Ω—ã–π —Ñ–æ–Ω–¥, –¥—Ä—É–∂–Ω—ã–π –∫–æ–ª–ª–µ–∫—Ç–∏–≤ –∏ –ø—Ä–æ—Ñ–µ—Å—Å–∏–æ–Ω–∞–ª—å–Ω–æ–µ –ø–æ–∫–µ—Ä —Å–æ–æ–±—â–µ—Å—Ç–≤–æ.\n\n&#128591<b>–ë–æ–ª–µ–µ 7 –ª–µ—Ç –æ–±—É—á–∞–µ–º –ø—Ä–æ—Ñ–µ—Å—Å–∏–æ–Ω–∞–ª—å–Ω–æ–º—É –ø–æ–∫–µ—Ä—É —Å –Ω—É–ª—è,</b> –æ–±–µ—Å–ø–µ—á–∏–≤–∞–µ–º –±—ç–∫–∏–Ω–≥ –∏ —Ä–æ—Å—Ç –∏–≥—Ä–æ–∫–æ–≤ –ø–æ –ª–∏–º–∏—Ç–∞–º –≤ —Ç–∞–∫–∏—Ö –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—è—Ö –∫–∞–∫ MTT, Spin&Go, MTSNG, Cash –∏ –∏–º–µ–µ–º –æ—Ç–ª–∏—á–Ω—É—é —Ä–µ–ø—É—Ç–∞—Ü–∏—é –≤ –ø–æ–∫–µ—Ä–Ω–æ–º –∫–æ–º—å—é–Ω–∏—Ç–∏.\n\n&#127891–ú—ã —Ä–∞–±–æ—Ç–∞–ª–∏ —Å –±–æ–ª–µ–µ —á–µ–º <b>1000 –∏–≥—Ä–æ–∫–∞–º–∏</b>\n\n&#128170<b>–£—Å–ø–µ—Ö–∏ –∏–≥—Ä–æ–∫–æ–≤</b> –∫–æ–º–∞–Ω–¥—ã FireStorm –≤–æ –≤—Å–µ—Ö –¥–∏—Å—Ü–∏–ø–ª–∏–Ω–∞—Ö –≥–æ–≤–æ—Ä—è—Ç —Å–∞–º–∏ –∑–∞ —Å–µ–±—è.\n\n&#9888<ins>–í –º–µ–Ω—é –Ω–∏–∂–µ, –≤—ã–±–µ—Ä–∏—Ç–µ –ø–æ–¥—Ö–æ–¥—è—â–µ–µ –≤–∞–º –≤ –Ω–∞–ø—Ä–∞–ª–µ–Ω–∏–µ</ins>&#9888', parse_mode='html' .format(message.from_user), reply_markup = markup)

@bot.message_handler(content_types=['text'])
def bot_message(message):
	if message.chat.type == 'private':
		if message.text == 'üòé1':
			bot.send_message(message.chat.id, '–í–∞—à–µ —á–∏—Å–ª–æ')

		elif message.text == 'ü§ï –î–ª—è –Ω–æ–≤–∏—á–∫–æ–≤':
			markup = types.ReplyKeyboardMarkup(resize_keyboard = True)
			item1 = types.KeyboardButton('–ü–æ–¥–∞—Ç—å –∑–∞—è–≤–∫—É')
			back = types.KeyboardButton('–ù–∞–∑–∞–¥')
			markup.add(item1, back)
			bot.send_message(message.chat.id,'<b>üî•–ù–µ –∑–Ω–∞–µ—à—å –∫–∞–∫–æ–µ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–∏ –≤—ã–±—Ä–∞—Ç—å?</b>\n\n‚ö†–û—Å—Ç–∞–≤—å –∑–∞—è–≤–∫—É –∏ –º–µ–Ω–µ–¥–∂–µ—Ä –æ–±—è–∑–∞—Ç–µ–ª—å–Ω–æ —Å–≤—è–∂–µ—Ç—Å—è —Å —Ç–æ–±–æ–π –∏ —Ä–∞—Å—Å–∫–∞–∂–µ—Ç:\n- –æ –≤—Å–µ—Ö –ø—Ä–µ–∏–º—É—â–µ—Å—Ç–≤–∞—Ö —Ç–æ–≥–æ –∏–ª–∏ –∏–Ω–æ–≥–æ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—è\n- –∞ —Ç–∞–∫ –∂–µ —Å–æ –≤–º–µ—Å—Ç–æ —Å —Ç—Ä–µ–Ω–µ—Ä–∞–º–∏ –ø–æ–º–æ–∂–µ—Ç –ø–æ–¥–æ–±—Ä–∞—Ç—å –¥–∏—Å—Ü–∏–ø–ª–∏–Ω—É –ø–æ–¥—Ö–æ–¥—è—â—É—é –∏–º–µ–Ω–Ω–æ —Ç–µ–±–µ\n\n<b>‚ùó–ü—Ä–æ–µ–∫—Ç TeamFirestorm –¥–∞–µ—Ç –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç—å:</b>\n- –Ω–µ —Ä–∏—Å–∫–æ–≤–∞—Ç—å —Å–≤–æ–∏–º–∏ –¥–µ–Ω—å–≥–∞–º–∏\n- –Ω–µ –ø–ª–∞—Ç–∏—Ç—å –∑–∞ –æ–±—É—á–µ–Ω–∏–µ\n- –º—ã –Ω–∞—á–∏–Ω–∞–µ–º –∑–∞—Ä–∞–±–∞—Ç—ã–≤–∞—Ç—å —Ç–æ–≥–¥–∞, –∫–æ–≥–¥–∞ –∑–∞—Ä–∞–±–∞—Ç—ã–≤–∞–µ—à—å —Ç—ã', parse_mode='html' .format(message.from_user), reply_markup = markup)


		elif message.text == 'üòé –î–ª—è –ø—Ä–æ–¥–≤–∏–Ω—É—Ç—ã—Ö':
			markup = types.ReplyKeyboardMarkup(resize_keyboard=True)
			item1 = types.KeyboardButton('–ü–æ–¥–∞—Ç—å –∑–∞—è–≤–∫—É')
			back = types.KeyboardButton('–ù–∞–∑–∞–¥')
			markup.add(item1, back)
			bot.send_message(message.chat.id,'üî•<b>–†–∞–±–æ—Ç–∞–µ–º —Å –∏–≥—Ä–æ–∫–∞–º–∏ –ø–æ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—è–º:\n Spin&Go, MTT, MTSNG, –∞ —Ç–∞–∫–∂–µ –∫—ç—à-–∏–≥—Ä—ã.</b>\n\n<ins>‚ö†–ß—Ç–æ_–º—ã_–ø—Ä–µ–¥–ª–∞–≥–∞–µ–º:</ins>\n- 100% –ø–æ–¥–¥–µ—Ä–∂–∫—É –Ω–∞ –∏–≥—Ä–∞–µ–º—ã–µ –¥–∏—Å—Ü–∏–ø–ª–∏–Ω—ã;\n- –±–µ—Å–ø–ª–∞—Ç–Ω–æ–µ –æ–±—É—á–µ–Ω–∏–µ —Å —Ç—Ä–µ–Ω–µ—Ä–∞–º–∏, –±—å—é—â–∏–º–∏ –≤–∞—à–∏ –ª–∏–º–∏—Ç—ã (–∏–ª–∏ –ª–∏–º–∏—Ç—ã –≤—ã—à–µ) —Å ROI 30+;\n- –±–æ–ª—å—à–∞—è –±–∞–∑–∞ –≤–∏–¥–µ–æ—É—Ä–æ–∫–æ–≤, –ø–æ–¥–≥–æ—Ç–æ–≤–ª–µ–Ω–Ω—ã—Ö —Å–ø–µ—Ü–∏–∞–ª—å–Ω–æ –¥–ª—è –∏–≥—Ä–æ–∫–æ–≤ –Ω–∞—à–µ–π –∫–æ–º–∞–Ω–¥—ã;\n- —Ä–∞–∑–Ω–æ–æ–±—Ä–∞–∑–Ω–∞—è –æ–Ω–ª–∞–π–Ω –∏ –æ—Ñ—Ñ–ª–∞–π–Ω-–∞–∫—Ç–∏–≤–Ω–æ—Å—Ç—å (—Å–æ–≤–º–µ—Å—Ç–Ω–∞—è —Ä–∞–±–æ—Ç–∞ –Ω–∞–¥ –∏–≥—Ä–æ–π, –ø–æ–º–æ—â—å –≤ —Ç—Ä—É–¥–Ω—ã–µ –º–æ–º–µ–Ω—Ç—ã –∂–∏–∑–Ω–∏, –æ—Ñ—Ñ–ª–∞–π–Ω-–≤—Å—Ç—Ä–µ—á–∏ –∏–≥—Ä–æ–∫–æ–≤ –∫–æ–º–∞–Ω–¥—ã –∏ –º–Ω–æ–≥–æ–µ –¥—Ä—É–≥–æ–µ.\n\nüí•–£ –Ω–∞—Å —Ä–∞–∑–Ω—ã–µ –≤–∞—Ä–∏–∞–Ω—Ç—ã —Å–æ—Ç—Ä—É–¥–Ω–∏—á–µ—Å—Ç–≤–∞ –ø–æ —Å–∏—Å—Ç–µ–º–µ win-win, —á—Ç–æ–±—ã –ø–æ–º–æ—á—å –∏–≥—Ä–æ–∫—É –¥–æ—Å—Ç–æ–π–Ω–æ –ø–æ–∫–æ—Ä—è—Ç—å –≤—ã—Å–æ–∫–∏–µ –ª–∏–º–∏—Ç—ã –≤ –ø–æ–∫–µ—Ä–µ',  parse_mode='html' .format(message.from_user), reply_markup = markup)

		elif message.text == 'üìã FAQ':

			bot.send_message(message.chat.id,'‚ùó<b>–ö–∞–∫–∏–µ —Ç—Ä–µ–±–æ–≤–∞–Ω–∏—è –¥–ª—è –≤—Å—Ç—É–ø–ª–µ–Ω–∏—è –≤ –∫–æ–º–∞–Ω–¥—É?</b>\n–£ –Ω–∞—Å –Ω–µ—Ç –∂–µ—Å—Ç–∫–∏—Ö —Ç—Ä–µ–±–æ–≤–∞–Ω–∏–π –∫ –≤—Å—Ç—É–ø–ª–µ–Ω–∏—é –≤ –∫–æ–º–∞–Ω–¥—É. –ü–æ–¥–∞—Ç—å –∑–∞—è–≤–∫—É –∏ —Å—Ç–∞—Ç—å –∏–≥—Ä–æ–∫–æ–º –∫–æ–º–∞–Ω–¥—ã –º–æ–∂–µ—Ç –ª—é–±–æ–π —á–µ–ª–æ–≤–µ–∫, –¥–∞–∂–µ —Ç–æ—Ç —É –∫–æ—Ç–æ—Ä–æ–≥–æ –Ω–µ—Ç –æ–ø—ã—Ç–∞ –≤ –ø–æ–∫–µ—Ä–µ.\n–ù–æ –Ω–∞–º –≤–∞–∂–Ω–æ, —á—Ç–æ–±—ã —É –í–∞—Å –ø—Ä–∏—Å—É—Ç—Å—Ç–≤–æ–≤–∞–ª–∏ —Ç–∞–∫–∏–µ –∫–∞—á–µ—Å—Ç–≤–∞, –∫–∞–∫: –∂–µ–ª–∞–Ω–∏–µ, —É—Å–µ—Ä–¥–∏–µ, —á–µ—Å—Ç–Ω–æ—Å—Ç—å.–í –ª—é–±–æ–º —Å–ª—É—á–∞–µ, –º—ã —Ä–∞—Å—Å–º–∞—Ç—Ä–∏–≤–∞–µ–º –≤—Å–µ –ø—Ä–∏—Å–ª–∞–Ω–Ω—ã–µ –∑–∞—è–≤–∫–∏.\n\n‚ùó<b>–ö–∞–∫–∏–µ –ø–æ–∫–µ—Ä–Ω—ã–µ –¥–∏—Å—Ü–∏–ø–ª–∏–Ω—ã –µ—Å—Ç—å –≤ –∫–æ–º–∞–Ω–¥–µ ?</b>\n–ú—ã –Ω–∞–±–∏—Ä–∞–µ–º –≤ –∫–æ–º–∞–Ω–¥—É –∏–≥—Ä–æ–∫–æ–≤ –≤ –¥–∏—Å—Ü–∏–ø–ª–∏–Ω—ã Mtt (sng), Spin&Go, Cash.\n\n‚ùó<b>–ù–∞ –∫–∞–∫–æ–π —Å—Ä–æ–∫ –∑–∞–∫–ª—é—á–∞–µ—Ç—Å—è –∫–æ–Ω—Ç—Ä–∞–∫—Ç?</b>\n–ö–∞–∫ –ø—Ä–∞–≤–∏–ª–æ, –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å –∫–æ–Ω—Ç—Ä–∞–∫—Ç–∞ —Å –∏–≥—Ä–æ–∫–æ–º –æ–±—Å—É–∂–¥–∞–µ—Ç—Å—è –∏–Ω–¥–∏–≤–∏–¥—É–∞–ª—å–Ω–æ, –ø–æ—Ç–æ–º—É —á—Ç–æ —Å—Ä–æ–∫ —Å–æ—Ç—Ä—É–¥–Ω–∏—á–µ—Å—Ç–≤–∞ –∑–∞–≤–∏—Å–∏—Ç –æ—Ç —É—Ä–æ–≤–Ω—è –∏–≥—Ä—ã –∫–∞–Ω–¥–∏–¥–∞—Ç–∞.\n–ü–æ–º–∏–º–æ —ç—Ç–æ–≥–æ, –µ—Å–ª–∏ —É –∏–≥—Ä–æ–∫–∞, –∏–∑–º–µ–Ω–∏–ª–∏—Å—å –æ–±—Å—Ç–æ—è—Ç–µ–ª—å—Å—Ç–≤–∞ –∏ –æ–Ω —Ö–æ—á–µ—Ç –∑–∞–∫–æ–Ω—á–∏—Ç—å —Å–æ—Ç—Ä—É–¥–Ω–∏—á–µ—Å—Ç–≤–æ, –º—ã –∏–¥–µ–º –Ω–∞ –≤—Å—Ç—Ä–µ—á—É.\n–¢–∞–∫–∂–µ –ø—Ä–∏—Å—É—Ç—Å—Ç–≤—É—é—Ç —Ç–µ—Å—Ç–æ–≤–∞—è –¥–∏—Å—Ç–∞–Ω—Ü–∏—è, –≥–¥–µ –∏–≥—Ä–æ–∫ –º–æ–∂–µ—Ç –ø–æ–ø—Ä–æ–±–æ–≤–∞—Ç—å —Å–≤–æ–∏ —Å–∏–ª—ã –≤ –∫–æ–º–∞–Ω–¥–µ –∏ —Ä–µ—à–∏—Ç—å –¥–ª—è —Å–µ–±—è, —Å—Ç–æ–∏—Ç –ø—Ä–æ–¥–æ–ª–∂–∞—Ç—å —Å–æ—Ç—Ä—É–¥–Ω–∏—á–µ—Å—Ç–≤–æ —Å Team Firestorm!\n\n‚ùó<b>–ö–∞–∫ –ø—Ä–æ—Ö–æ–¥–∏—Ç –æ–±—É—á–µ–Ω–∏–µ –≤ –∫–æ–º–∞–Ω–¥–µ?</b>\n–û–±—É—á–µ–Ω–∏–µ –∫–∞–∂–¥–æ–º—É –∏–≥—Ä–æ–∫—É –º—ã –ø–æ–¥–±–∏—Ä–∞–µ–º –∏–Ω–¥–∏–≤–∏–¥—É–∞–ª—å–Ω–æ –∏—Å—Ö–æ–¥—è –∏–∑ —É—Ä–æ–≤–Ω—è –∏–≥—Ä—ã –∫–∞–Ω–¥–∏–¥–∞—Ç–∞, –Ω–∞—á–∏–Ω–∞—è —Å –≥—Ä—É–ø–ø–æ–≤—ã—Ö –∏ –∑–∞–∫–∞–Ω—á–∏–≤–∞—è –∏–Ω–¥–∏–≤–∏–¥—É–∞–ª—å–Ω—ã–º–∏, –ø–æ—Ç–æ–∫–æ–≤—ã–º–∏ —Ç—Ä–µ–Ω–∏—Ä–æ–≤–∫–∞–º–∏.\n–ü–æ–º–∏–º–æ –∏–Ω–¥–∏–≤–∏–¥—É–∞–ª—å–Ω–æ –ø–æ–¥–æ–±—Ä–∞–Ω–Ω–æ–≥–æ –æ–±—É—á–µ–Ω–∏—è –∫–∞–∂–¥—ã–π –∏–≥—Ä–æ–∫ –ø–æ–ª—É—á–∞–µ—Ç –¥–æ—Å—Ç—É–ø –∫ –±–∞–∑–µ –í–∏–¥–µ–æ —Ç—Ä–µ–Ω–∏—Ä–æ–≤–æ–∫, –º–æ–∂–µ—Ç –ø–æ—Å–µ—â–∞—Ç—å –æ—Ç–∫—Ä—ã—Ç—ã–µ —Ç—Ä–µ–Ω–∏—Ä–æ–≤–∫–∏, –∫–æ—Ç–æ—Ä—ã–µ –ø—Ä–æ—Ö–æ–¥—è—Ç –≤ —Ñ–æ—Ä–º–∞—Ç–µ –ª–µ–∫—Ü–∏–π.\n–¢–∞–∫–∂–µ –ø–æ–ª—É—á–∞—é—Ç –¥–æ—Å—Ç—É–ø –∫ —Ä–∞–±–æ—Ç–µ —Å –Ω–∞—Å—Ç–∞–≤–Ω–∏–∫–∞–º–∏, –ø—Å–∏—Ö–æ–ª–æ–≥–∞–º–∏, –º–µ–Ω—Ç–∞–ª –∫–æ—É—á–∞–º–∏ –∫–æ–º–∞–Ω–¥—ã.\n\n‚ùó<b>–ú–æ–∂–Ω–æ –ø—Ä–æ–π—Ç–∏ –æ–±—É—á–µ–Ω–∏–µ –≤ –∫–æ–º–∞–Ω–¥–µ –±–µ–∑ –±–µ–∫–∏–Ω–≥–∞?</b>\n–û–±—É—á–µ–Ω–∏–µ –∏ –±–µ–∫–∏–Ω–≥ –Ω–µ—Ä–∞–∑–¥–µ–ª–∏–º—ã. –ù–æ —É –Ω–∞—Å –µ—Å—Ç—å –±–µ—Å–ø–ª–∞—Ç–Ω—ã–µ –∫—É—Ä—Å, –∫–æ—Ç–æ—Ä—ã–µ –º–æ–∂–µ—Ç –ø—Ä–æ–π—Ç–∏ –ª—é–±–æ–π –∂–µ–ª–∞—é—â–∏–π. –î–ª—è –ø–æ–ª—É—á–µ–Ω–∏—è –¥–æ—Å—Ç—É–ø–∞ –∫ –∫—É—Ä—Å—É –ø–µ—Ä–µ–π–¥–∏—Ç–µ –ø–æ —Å—Å—ã–ª–∫–µ.\n\n‚ùó<b>–í –∫–∞–∫–∏—Ö –ø–æ–∫–µ—Ä —Ä—É–º–∞—Ö –º–æ–∂–Ω–æ –∏–≥—Ä–∞—Ç—å?</b>–ò–≥—Ä–∞—Ç—å –º–æ–∂–Ω–æ –≤ –ª—é–±–æ–º –ø–æ–∫–µ—Ä —Ä—É–º–µ. –ì–ª–∞–≤–Ω–æ–µ, —á—Ç–æ–±—ã –∏–≥—Ä–∞ –≤ —Ä—É–º–µ –±—ã–ª–∞ –ª–µ–≥–∞–ª—å–Ω–æ–π –≤ —Ç–æ–π —Å—Ç—Ä–∞–Ω–µ, –≥–¥–µ –ø—Ä–æ–∂–∏–≤–∞–µ—Ç –∏–≥—Ä–æ–∫.\n–ù–∏–∫–∞–∫–∏—Ö VPN.\n–ü–æ–¥–±–æ—Ä–æ–º –∏–≥—Ä–æ–≤—ã—Ö —Ä—É–º–æ–≤ –∑–∞–Ω–∏–º–∞–µ—Ç—Å—è –∫–æ–º–∞–Ω–¥–∞ –∏—Å—Ö–æ–¥—è –∏–∑ –ø–æ–∫–µ—Ä–Ω–æ–π –¥–∏—Å—Ü–∏–ø–ª–∏–Ω—ã, –∫–æ—Ç–æ—Ä—É—é –í—ã –∏–≥—Ä–∞–µ—Ç–µ –≤ Team Firestorm! –ü—Ä–∏ –ø–æ–¥–±–æ—Ä–µ —Ä—É–º–æ–≤ –≤—Å–µ –ø–æ–∂–µ–ª–∞–Ω–∏—è –∏–≥—Ä–æ–∫–∞ —É—á–∏—Ç—ã–≤–∞—é—Ç—Å—è!\n\n‚ùó<b>–ü–æ–∫–µ—Ä–Ω—ã–π —Å–æ—Ñ—Ç –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª—è–µ—Ç–µ?</b>\n–ö–æ–º–∞–Ω–¥–∞ –ø—Ä–µ–¥–æ—Å—Ç–∞–≤–ª—è–µ—Ç –¥–æ—Å—Ç—É–ø –∫ —É–¥–∞–ª–µ–Ω–Ω–æ–º —Å–µ—Ä–≤–µ—Ä—É –Ω–∞ –∫–æ—Ç–æ—Ä–æ–º –µ—Å—Ç—å –≤–µ—Å—å –ø–æ–∫–µ—Ä–Ω—ã–π —Å–æ—Ñ—Ç –¥–ª—è —Ä–∞–±–æ—Ç—ã –Ω–∞–¥ –∏–≥—Ä–æ–π.\n\n‚ùó<b>–ß—Ç–æ —è –µ—â–µ –ø–æ–ª—É—á—É –≤—Å—Ç—É–ø–∏–≤ –≤ –∫–æ–º–∞–Ω–¥—É?</b>\n–ö–∞—á–µ—Å—Ç–≤–µ–Ω–Ω–æ–µ –æ–±—É—á–µ–Ω–∏–µ –æ—Ç —Ç—Ä–µ–Ω–µ—Ä–æ–≤ —Ñ–æ–Ω–¥–∞ –∏ –ø—Ä–∏–≥–ª–∞—à–µ–Ω–Ω—ã—Ö —Ç–æ–ø–æ–≤—ã—Ö —Ä–µ–≥—É–ª—è—Ä–æ–≤.\n–í—Å–µ –≤–∏–¥–µ–æ —Å –ª–µ–∫—Ü–∏–π –∏ –≤–µ–±–∏–Ω–∞—Ä–æ–≤ —Å–æ—Ö—Ä–∞–Ω—è—é—Ç—Å—è –æ–Ω–ª–∞–π–Ω –≤ –Ω–∞—à–µ–π –±–∏–±–ª–∏–æ—Ç–µ–∫–µ, –¥–æ—Å—Ç—É–ø –∫ –∫–æ—Ç–æ—Ä–æ–π –µ—Å—Ç—å —É –≤—Å–µ—Ö —á–ª–µ–Ω–æ–≤ –∫–æ–º–∞–Ω–¥—ã. –í –∞—Ä—Ö–∏–≤–µ –º–æ–∂–Ω–æ –Ω–∞–π—Ç–∏ –º–∞—Ç–µ—Ä–∏–∞–ª—ã –Ω–∞ –ª—é–±—É—é –ø–æ–∫–µ—Ä–Ω—É—é —Ç–µ–º—É –æ—Ç —Ä–∞–∑–Ω—ã—Ö —Ç—Ä–µ–Ω–µ—Ä–æ–≤. –ë–∞–∑–∞ –ø–æ—Å—Ç–æ—è–Ω–Ω–æ –ø–æ–ø–æ–ª–Ω—è–µ—Ç—Å—è.\n–ü–∞—Ä–∞–ª–ª–µ–ª—å–Ω–æ —Å –±–µ–∫–∏–Ω–≥–æ–º –∏ –æ–±—É—á–µ–Ω–∏–µ–º –≤—ã –ø–æ–ª—É—á–∞–µ—Ç–µ –¥–æ—Å—Ç—É–ø –∫ –∑–∞–∫—Ä—ã—Ç—ã–º –∫–æ–Ω—Ñ–µ—Ä–µ–Ω—Ü–∏—è–º —Å –∏–≥—Ä–æ–∫–∞–º–∏, —Ç—Ä–µ–Ω–µ—Ä–∞–º–∏ –∫–æ–º–∞–Ω–¥—ã, –≥–¥–µ –º–æ–∂–µ—Ç–µ –æ–±—â–∞—Ç—å—Å—è –Ω–∞ –ª—é–±—ã–µ –ø–æ–∫–µ—Ä–Ω—ã–µ –∏ –æ–∫–æ–ª–æ –ø–æ–∫–µ—Ä–Ω—ã–µ —Ç–µ–º—ã, —Ä–∞–∑–±–∏—Ä–∞—Ç—å —Ä–∞–∑–¥–∞—á–∏ –∏ –∑–∞–¥–∞–≤–∞—Ç—å –≤–æ–ø—Ä–æ—Å—ã —Ç—Ä–µ–Ω–µ—Ä–∞–º.\n–¢–∞–∫–∂–µ —É—á–∞—Å—Ç–≤–æ–≤–∞—Ç—å –≤ —Ä–∞–∑–Ω—ã—Ö –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏—è—Ö –∫–æ–º–∞–Ω–¥—ã, –ø—Ä–æ–≤–æ–¥–∏—Ç—å –ø—Ä–∏—è—Ç–Ω–æ –¥–æ—Å—É–≥ —Å –¥—Ä—É–≥–∏–º–∏ —É—á–∞—Å—Ç–Ω–∏–∫–∞–º–∏ –∫–æ–º–∞–Ω–¥—ã, –≤–º–µ—Å—Ç–µ –ø—É—Ç–µ—à–µ—Å—Ç–≤–æ–≤–∞—Ç—å, –ø–æ—Å–µ—â–∞—Ç—å –æ—Ñ—Ñ–ª–∞–π–Ω –≤—Å—Ç—Ä–µ—á–∏ –∫–æ–º–∞–Ω–¥—ã.\n\n‚ùó<b>–ö–æ–≥–¥–∞ —è –±—É–¥—É –ø–æ–ª—É—á–∞—Ç—å —Å–≤–æ–π –æ—Ç–∫–∞—Ç?</b>\n–ó–∞ –∫–∞–∂–¥—ã–º –∏–≥—Ä–æ–∫–æ–º –∑–∞–∫—Ä–µ–ø–ª–µ–Ω–∞ –¥–∞—Ç–∞ –∑–∞–∫—Ä—ã—Ç–∏—è –ø–µ—Ä–∏–æ–¥–∞ (—Å–µ—Ç–∞) –∏ –µ–∂–µ–º–µ—Å—è—á–Ω–æ –≤ —ç—Ç–æ—Ç –¥–µ–Ω—å –∏–≥—Ä–æ–∫ –ø–æ–ª—É—á–∞–µ—Ç —Å–≤–æ–π –æ—Ç–∫–∞—Ç.\n–ß–∞—Å—Ç–æ –∏–≥—Ä–æ–∫–∏ –ø—Ä–æ—Å—è—Ç –ø—Ä–æ–≤–µ—Å—Ç–∏ —Ä–∞—Å—á–µ—Ç –ø–æ—Å–ª–µ –∫—Ä—É–ø–Ω–æ–≥–æ –≤—ã–∏–≥—Ä—ã—à–∞ –∏ –≤—ã–≤–æ–¥—è—Ç —Å—Ä–µ–¥—Å—Ç–≤–∞ –≤ —Ç–æ—Ç –∂–µ –¥–µ–Ω—å.\n\n‚ùó<b>–°–∫–æ–ª—å–∫–æ –∏–≥—Ä–∞—Ç—å —Å—Ç–æ–ª–æ–≤?</b>\n–ù–∞ —Å—Ç–∞—Ä—Ç–µ –ª—É—á—à–µ –∏–≥—Ä–∞—Ç—å, —Ç–æ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —Å—Ç–æ–ª–æ–≤, –∫–æ—Ç–æ—Ä–æ–µ –¥–ª—è —Ç–µ–±—è –º–∞–∫—Å–∏–º–∞–ª—å–Ω–æ —É–¥–æ–±–Ω–æ–µ –∏ –∫–æ–º—Ñ–æ—Ä—Ç–Ω–æ–µ. –ê –¥–∞–ª–µ–µ –∏–Ω–¥–∏–≤–∏–¥—É–∞–ª—å–Ω–æ —Å–º–æ—Ç—Ä–µ—Ç—å –∏ –¥–æ–±–∞–≤–ª—è—Ç—å –∏–ª–∏ —É–±–∏—Ä–∞—Ç—å.',  parse_mode='html' .format(message.from_user) )

		elif message.text == '–ù–∞–∑–∞–¥':
			markup = types.ReplyKeyboardMarkup(resize_keyboard=True, row_width= 2)
			item1 = types.KeyboardButton('ü§ï –î–ª—è –Ω–æ–≤–∏—á–∫–æ–≤')
			item2 = types.KeyboardButton('üòé –î–ª—è –ø—Ä–æ–¥–≤–∏–Ω—É—Ç—ã—Ö')
			item3 = types.KeyboardButton('üìã FAQ')
			markup.add(item1, item2, item3)
			bot.send_message(message.chat.id, '–ù–∞–∑–∞–¥', reply_markup=markup)

		elif message.text == '–ü–æ–¥–∞—Ç—å –∑–∞—è–≤–∫—É':

			bot.send_message(message.chat.id, 'https://teamfirestorm.taplink.ws/')

bot.polling(none_stop=True)
